#include <iostream>

using namespace std;
int pow(int a, int b);

int main(){
    int a,b,ans;
    cout << "enter the value of base and power : " ;
    cin >> a >> b;
    ans = pow(a,b);   // fun calling 
    cout << "Answer is : " << ans << endl;
}

int pow(int a, int b){
    int ans = 1;
    for(int i=1; i<=b; i++){
        ans = ans*a;
    }
    return ans;
}
