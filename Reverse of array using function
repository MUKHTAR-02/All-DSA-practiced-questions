#include <iostream>
using namespace std;
void arrayRev(int n);
void arrayRev(int n){
    // int n;
    // cout << "enter the size of array : ";
    // cin >> n;
    int arr[n];
    cout << "enter the elements of array : " << endl;
    for(int i=0; i<n; i++){
        cin >> arr[i];
    }
    // int sum = 0;
    cout << "reverse of array : " << endl;
    for(int i=n-1; i>=0; i--){
        cout << arr[i] << " ";
    }
}
int main() {
    int n;
    cout << "enter the size of array : " <<endl;
    cin >> n;
    arrayRev(n);
    return 0;
}
