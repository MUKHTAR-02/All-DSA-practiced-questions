#include<iostream>
using namespace std;
// void array(int n, int arr[]);
void array(int arr[],int n){
    cout << "inside funtion" << endl;
    for(int i=0; i<n; i++){
        cin >> arr[i];
    }
    cout << endl;
    for(int i=0; i<n; i++){
        cout << arr[i] << " ";
    }
}
int main()
{
    cout << "main called" << endl;
    int n;
    cout << "enter the size of array : ";
    cin >> n;
    int arr[n];
    array(arr,n);
}
