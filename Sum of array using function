#include <iostream>
using namespace std;
int arraySum();
int arraySum(){
    int n;
    cout << "enter the size of array : ";
    cin >> n;
    int arr[n];
    cout << "enter the elements of array : " << endl;
    for(int i=0; i<n; i++){
        cin >> arr[i];
    }
    int sum = 0;
    for(int i=0; i<n; i++){
        sum = sum + arr[i];
    }
    return sum;
}
int main() {
    
    int sum = arraySum();
    cout << "sum of array : " << sum;
    return 0;
}
